  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": [
    "api",
    "latency"
  ],
  "templating": {
    "list": [
      {
        "current": {
          "selected": true,
          "text": "All",
          "value": ".*"
