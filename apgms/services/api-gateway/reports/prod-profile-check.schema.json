{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Prod profile check report",
  "type": "object",
  "required": ["timestamp", "results"],
  "properties": {
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "results": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "required": ["pass", "detail"],
        "properties": {
          "pass": {
            "type": "boolean"
          },
          "detail": {
            "type": "string"
          }
        }
      }
    }
  }
}
