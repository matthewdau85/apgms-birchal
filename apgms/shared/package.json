{
  "name": "@apgms/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "files": ["dist", "prisma"],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && pnpm run prisma:generate && tsc -p tsconfig.build.json",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@prisma/client": "6.17.1"
  },
  "devDependencies": {
    "prisma": "6.17.1",
    "typescript": "^5.9.3"
  }
}
